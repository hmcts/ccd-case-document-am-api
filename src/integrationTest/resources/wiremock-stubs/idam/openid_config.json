{
  "request": {
    "method": "GET",
    "url": "/o/.well-known/openid-configuration"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "issuer": "http://localhost:{{request.requestLine.port}}/o",
      "jwks_uri": "http://localhost:{{request.requestLine.port}}/jwks"
    },
    "transformers": ["response-template"]
  }
}

