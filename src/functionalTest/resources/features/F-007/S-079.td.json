{
  "title": "must successfully validate hashed-token for multiple documents",
  "_guid_": "S-079",
  "_extends_": "F-007_Test_Data_Base",

  "specs": [
    "contains a Case Id along with ids of the documents uploaded above",
    "is to be made on behalf of CCD Data Store API"
  ],

  "request": {
    "body": {
      "caseId": "1234567890099900",
      "caseTypeId" : "${[scenarioContext][childContexts][F-007-Upload_Document1][testData][actualResponse][body][_embedded][documents][0][metadata][caseTypeId]}",
      "jurisdictionId" : "${[scenarioContext][childContexts][F-007-Upload_Document1][testData][actualResponse][body][_embedded][documents][0][metadata][jurisdictionId]}",
      "documentHashTokens": [
        {
          "id": "${[scenarioContext][childContexts][F-007-Upload_Document1][customValues][documentIdInTheResponse]}",
          "hashToken": "${[scenarioContext][childContexts][F-007-Upload_Document1][testData][actualResponse][body][_embedded][documents][0][hashToken]}"
        },
        {
          "id": "${[scenarioContext][childContexts][F-007-Upload_Document2][customValues][documentIdInTheResponse]}",
          "hashToken": "${[scenarioContext][childContexts][F-007-Upload_Document2][testData][actualResponse][body][_embedded][documents][0][hashToken]}"
        },
        {
          "id": "${[scenarioContext][childContexts][F-007-Upload_Document3][customValues][documentIdInTheResponse]}",
          "hashToken": "${[scenarioContext][childContexts][F-007-Upload_Document3][testData][actualResponse][body][_embedded][documents][0][hashToken]}"
        }
      ]
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response"
  }
}
