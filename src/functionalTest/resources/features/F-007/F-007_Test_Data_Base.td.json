{
  "_guid_": "F-007_Test_Data_Base",
  "_extends_": "F-007-Base-Root",
  "productName": "CCD Case Document AM API",
  "operationName": "Attach Documents To Case",
  "apiClientId": "ccd_data",
  "specs": [
    "an active caseworker profile in CCD with full permissions on a document field"
  ],
  "method": "PATCH",
  "uri": "/cases/documents/attachToCase",
  "request": {
    "body": {
      "caseId": "1234567890099900",
      "caseTypeId": "${[scenarioContext][childContexts][F-007-Upload_Document1][testData][actualResponse][body][documents][0][metadata][caseTypeId]}",
      "jurisdictionId": "${[scenarioContext][childContexts][F-007-Upload_Document1][testData][actualResponse][body][documents][0][metadata][jurisdictionId]}"
    }
  }
}
