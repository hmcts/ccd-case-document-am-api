{
  "title": "must receive an error response when Bulk Scan Processor tries to access Get Document Binary Content API",
  "_guid_": "S-115",
  "_extends_": "F-002_Test_Data_Base",
  "apiClientId": "bulk_scan_processor",
  "specs": [
    "is to be made on behalf of Bulk Scan Processor API"
  ],
  "request": {
    "pathVariables": {
      "documentId": "${[scenarioContext][childContexts][Default_Document_Upload_Data][customValues][documentIdInTheResponse]}"
    }
  },
  "expectedResponse": {
    "_extends_": "Common_403_Response",
    "headers": {
      "Content-Type": "application/json",
      "Content-Length": "[[ANYTHING_PRESENT]]"
    },
    "body": {
      "message": "Forbidden: Insufficient permissions: ${[scenarioContext][testData][request][pathVariables][documentId]}",
      "path": "/cases/documents/${[scenarioContext][testData][request][pathVariables][documentId]}/binary"
    }
  }
}
