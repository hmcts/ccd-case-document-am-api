  "title": "must upload a document successfully and get the response",
  "productName": "CCD Case Document AM API",
  "operationName": "upload a document with mandatory metadata",
  "method": "POST",
  "uri": "/cases/documents",
  "specs": [
    "uses a uid that exists in IDAM"
  ],
  "request": {
    "body": {
      "arrayInMap": [
        {
          "key": "classification",
          "value": "PUBLIC"
        },
        {
          "key": "caseTypeId",
          "value": "BEFTA_CASETYPE_2_2"
        },
        {
          "key": "jurisdictionId",
          "value": "BEFTA_JURISDICTION_2"
        },
        {
          "key": "files",
          "filePath": "SampleImage.png"
        }
      ]
    }
  },
  "expectedResponse": {
    "responseCode": 200,
    "responseMessage": "OK",
    "headers": {
      "Cache-Control" : "[[ANYTHING_PRESENT]]",
